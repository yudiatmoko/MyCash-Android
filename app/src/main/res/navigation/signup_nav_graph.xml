<?xml version="1.0" encoding="utf-8"?><navigation xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:id="@+id/signup_nav"    app:startDestination="@id/signUpFragment">    <fragment        android:id="@+id/signUpFragment"        android:name="com.iyam.mycash.ui.signup.SignUpFragment"        android:label="SignUpFragment">        <action            android:id="@+id/action_signUpFragment_to_otpConfirmFragment"            app:destination="@id/otpConfirmFragment" />        <action            android:id="@+id/action_signUpFragment_to_signInActivity"            app:destination="@id/signInActivity" />    </fragment>    <fragment        android:id="@+id/otpConfirmFragment"        android:name="com.iyam.mycash.ui.signup.OtpConfirmFragment"        android:label="OtpConfirmFragment">        <argument            android:name="auth"            app:argType="com.iyam.mycash.model.Auth"            app:nullable="true" />        <action            android:id="@+id/action_otpConfirmFragment_to_mainActivity"            app:destination="@id/mainActivity" />    </fragment>    <activity        android:id="@+id/signInActivity"        android:name="com.iyam.mycash.ui.signin.SignInActivity"        android:label="activity_sign_in"        tools:layout="@layout/activity_sign_in" />    <activity        android:id="@+id/mainActivity"        android:name="com.iyam.mycash.ui.main.MainActivity"        android:label="activity_main"        tools:layout="@layout/activity_main" /></navigation>