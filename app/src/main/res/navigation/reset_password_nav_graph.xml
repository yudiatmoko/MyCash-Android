<?xml version="1.0" encoding="utf-8"?><navigation xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:id="@+id/reset_password_nav_graph"    app:startDestination="@id/inputEmailFragment">    <fragment        android:id="@+id/inputEmailFragment"        android:name="com.iyam.mycash.ui.resetpassword.InputEmailFragment"        android:label="fragment_input_email"        tools:layout="@layout/fragment_input_email" >        <action            android:id="@+id/action_inputEmailFragment_to_inputNewPasswordFragment"            app:destination="@id/inputNewPasswordFragment" />    </fragment>    <fragment        android:id="@+id/inputNewPasswordFragment"        android:name="com.iyam.mycash.ui.resetpassword.InputNewPasswordFragment"        android:label="fragment_input_new_password"        tools:layout="@layout/fragment_input_new_password" >        <action            android:id="@+id/action_inputNewPasswordFragment_to_signInActivity2"            app:destination="@id/signInActivity2" />        <argument            android:name="email"            app:argType="string" />    </fragment>    <activity        android:id="@+id/signInActivity2"        android:name="com.iyam.mycash.ui.signin.SignInActivity"        android:label="activity_sign_in"        tools:layout="@layout/activity_sign_in" /></navigation>