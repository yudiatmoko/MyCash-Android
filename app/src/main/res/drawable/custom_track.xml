<?xml version="1.0" encoding="utf-8"?><selector xmlns:android="http://schemas.android.com/apk/res/android">    <item android:state_checked="true">        <shape android:shape="rectangle">            <solid android:color="@color/md_dark_onPrimaryContainer" />            <corners android:radius="40dp" />            <size android:width="48dp" android:height="24dp" />        </shape>    </item>    <item android:state_checked="false">        <shape android:shape="rectangle">            <solid android:color="@color/md_dark_onSurface" />            <corners android:radius="40dp" />            <size android:width="48dp" android:height="24dp" />        </shape>    </item></selector>